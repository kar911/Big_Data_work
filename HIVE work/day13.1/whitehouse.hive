select * from wh_visits where time_of_arrival != "" order by unix_timestamp(time_of_arrival, 'MM/dd/yyyy hh:mm') limit 10;


select * from wh_visits where time_of_arrival != "" order by unix_timestamp(time_of_arrival, 'MM/dd/yyyy hh:mm') desc limit 10;


from wh_visits
select count(*) as comment_count, info_comment
group by info_comment
order by comment_count DESC
limit 10;
